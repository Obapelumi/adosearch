{
  "name": "adosearch",
  "version": "0.0.95",
  "description": "typed, deeply nested database search for adonis.js applications",
  "main": "build/providers/AdosearchProvider.js",
  "typings": "./build/adonis-typings/index.d.ts",
  "files": [
    "build/adonis-typings",
    "build/providers",
    "build/src"
  ],
  "scripts": {
    "pretest": "npm run lint",
    "test": "node -r @adonisjs/require-ts/build/register bin/test.ts",
    "build": "npm run compile",
    "format": "prettier --w .",
    "mrm": "mrm --preset=@adonisjs/mrm-preset",
    "clean": "del-cli build",
    "compile": "npm run clean && tsc",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Obapelumi/adosearch.git"
  },
  "keywords": [
    "search",
    "adonis.js",
    "database",
    "node.js",
    "relational",
    "database"
  ],
  "author": "obapelumi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Obapelumi/adosearch/issues"
  },
  "homepage": "https://github.com/Obapelumi/adosearch#readme",
  "prettier": {
    "trailingComma": "es5",
    "semi": false,
    "singleQuote": true,
    "useTabs": false,
    "quoteProps": "consistent",
    "bracketSpacing": true,
    "arrowParens": "always",
    "printWidth": 100
  },
  "peerDependencies": {
    "@adonisjs/core": "^5.8.4",
    "@adonisjs/lucid": "^18.1.0"
  },
  "devDependencies": {
    "@adonisjs/core": "^5.8.4",
    "@adonisjs/lucid": "^18.1.0",
    "@adonisjs/mrm-preset": "^5.0.3",
    "@adonisjs/require-ts": "^2.0.12",
    "@types/node": "^18.0.0",
    "del-cli": "^4.0.1",
    "mrm": "^4.0.0",
    "prettier": "^2.7.1",
    "typescript": "^4.7.4"
  },
  "mrmConfig": {
    "core": false,
    "license": "MIT",
    "services": [
      "github-actions"
    ],
    "minNodeVersion": "16.13.1",
    "probotApps": [],
    "runGhActionsOnWindows": false
  },
  "adonisjs": {
    "types": "adosearch",
    "providers": [
      "adosearch"
    ]
  }
}
