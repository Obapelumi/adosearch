{
  "name": "adosearch",
  "version": "0.1.93",
  "description": "typed, deeply nested database search for adonis.js applications",
  "main": "build/src/search.js",
  "typings": "./build/src/search.d.ts",
  "scripts": {
    "pretest": "npm run lint",
    "test": "node -r @adonisjs/require-ts/build/register bin/test.ts",
    "build": "npm run compile",
    "format": "prettier --write .",
    "mrm": "mrm --preset=@adonisjs/mrm-preset",
    "clean": "del-cli build",
    "compile": "npm run lint && npm run clean && tsc",
    "prepublishOnly": "npm run build",
    "lint": "eslint . --ext=.ts",
    "commit": "git-cz",
    "release": "np --message=\"chore(release): %s\"",
    "version": "npm run build",
    "sync-labels": "github-label-sync --labels ./node_modules/@adonisjs/mrm-preset/gh-labels.json Obapelumi/adosearch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Obapelumi/adosearch.git"
  },
  "keywords": [
    "search",
    "adonis.js",
    "database",
    "node.js",
    "relational",
    "database"
  ],
  "author": "obapelumi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Obapelumi/adosearch/issues"
  },
  "homepage": "https://github.com/Obapelumi/adosearch#readme",
  "prettier": {
    "trailingComma": "es5",
    "semi": false,
    "singleQuote": true,
    "useTabs": false,
    "quoteProps": "consistent",
    "bracketSpacing": true,
    "arrowParens": "always",
    "printWidth": 100
  },
  "peerDependencies": {
    "@adonisjs/core": "^5.8.4",
    "@adonisjs/lucid": "^18.1.0"
  },
  "devDependencies": {
    "@adonisjs/core": "^5.8.4",
    "@adonisjs/lucid": "^18.1.0",
    "@adonisjs/mrm-preset": "^5.0.3",
    "@adonisjs/require-ts": "^2.0.12",
    "@types/node": "^18.0.0",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "del-cli": "^4.0.1",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-adonis": "^2.1.1",
    "eslint-plugin-prettier": "^4.2.1",
    "github-label-sync": "^2.3.1",
    "husky": "^8.0.3",
    "mrm": "^4.0.0",
    "np": "^8.0.4",
    "prettier": "^2.7.1",
    "typescript": "^4.7.4"
  },
  "mrmConfig": {
    "core": false,
    "license": "MIT",
    "services": [
      "github-actions"
    ],
    "minNodeVersion": "16.13.1",
    "probotApps": [],
    "runGhActionsOnWindows": false
  },
  "files": [
    "build/src",
    "build/index.d.ts",
    "build/index.js"
  ],
  "eslintConfig": {
    "extends": [
      "plugin:adonis/typescriptPackage",
      "prettier"
    ],
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "endOfLine": "auto"
        }
      ]
    }
  },
  "eslintIgnore": [
    "build"
  ],
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "np": {
    "contents": ".",
    "anyBranch": false
  },
  "dependencies": {
    "@poppinss/utils": "^5.0.0",
    "luxon": "^3.4.2"
  }
}
